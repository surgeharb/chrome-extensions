{
  "manifest_version": 2,
 
  "name": "Best GitHub Dark Mode",
  "version": "1.0.0",
  "description": "Dark Mode Extension for GitHub",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": ["https://github.com/*"]
  }],

  "icons": { 
    "32": "icons/GitHub-Mark-32px.png",
    "64": "icons/GitHub-Mark-64px.png",
    "128": "icons/GitHub-Mark-120px-plus.png"
  }

}